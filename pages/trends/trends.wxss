.Cantop {
    width: 100%;
    height: 45px;
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
}

.Cantop>image{
  width: 96px;
  height: 30px;
}

.myNew {
    display: -webkit-box;
    padding-bottom: 10px;
    border-bottom: 2px solid #f9f9f9;
}

.newImage {
    width: 110px;
    height: 90px;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    border-radius: 10px;
		margin-top:5px;
}

.newDetails {
    -webkit-box-flex: 1;
    padding-left: 5px;
}

.newTitle {
    width: 100%;
    font-size: 30rpx;
    font-weight: bold;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    height: 30px;
    line-height: 30px;
}

.decs {
    font-size: 22rpx;
    color: #8a8a8a;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.time {
    color: #8a8a8a;
    font-size: 26rpx;
    margin-top: 10px;
}

.myNav {
    display: -webkit-box;
    -webkit-box-align: center;
    height: 32px;
    background-color: #f8f8f8;
}

.myNav>view {
    width: 80px;
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    height: 28px;
    font-size: 28rpx;
}

.newHome {
    padding:10px 15px 0 15px;
}

.navs {
    color: #363636;
    height: 28px;
    line-height: 28px;
    font-size: 28rpx;
    border-bottom: 2px solid #8a8a8a;
}

.seex {
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    height: 30px;
}

.seex>view:nth-child(2) {
    width: 20%;
    color: #c7c7c7;
    font-size:24rpx;
    text-align: center;
}

.seex>view:last-child, .seex>view:first-child {
    height: 1px;
    background-color: #dedede;
    -webkit-box-flex: 1;
    margin-left: 3%;
    margin-right: 3%;
}
